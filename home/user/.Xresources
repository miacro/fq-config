!-*-{ Xft settings
Xft*dpi:              96
Xft*antialias:        True
Xft*hinting:          Full
!-*-}

XTerm*VT100.geometry:     1920x1080
XTerm*VT100.DynamicColors: true
XTerm*VT100.Utf8: 2
XTerm*VT100.EightBitInput: true
XTerm*VT100.SaveLines: 40960
XTerm*VT100.scrollKey: false
XTerm*VT100.scrollTtyOutput: false
XTerm*VT100.scrollBar: false
XTerm*VT100.rightScrollBar: false
XTerm*VT100.JumpScroll: true
XTerm*VT100.MultiScroll: true
XTerm*ToolBar: false

XTerm*VT100.translations: #override <Btn1Up>: select-end(PRIMARY, CLIPBOARD, CUT_BUFFER0)
XTerm*VT100.Translations: #override \n\
        Ctrl Shift <KeyPress> C:  select-end(CLIPBOARD,PRIMARY,CUT_BUFFER0) \n\
        <BtnUp>: select-end(CLIPBOARD,PRIMARY,CUT_BUFFER0) \n\
        Ctrl Shift <KeyPress> V: insert-selection(CLIPBOARD,PRIMARY,CUT_BUFFER0) \n\
        <Btn2Down> : insert-selection(CLIPBOARD,PRIMARY,CUT_BUFFER0) \n\
        <Key>Insert:            string(/001) \n\
        Shift <Key>Up:          scroll-back(1,lines) \n\
        Shift <Key>Down:        scroll-forw(1,lines) \n\


!XTerm font and locale
XTerm*VT100.Locale: true
XTerm*VT100.Utf8Title: true
XTerm*fontMenu*fontdefault*Label: Default
XTerm*faceName: Source Code Pro:antialias=true:pixelsize=16
!XTerm*faceName: DejaVu Sans Mono:antialias=true:pixelsize=16
!XTerm*faceName: Consolas:antialias=true:pixelsize=18:italic
XTerm*faceNameDoublesize: Source Han Sans CN:pixelsize=16:antialias=true
!XTerm*faceNameDoublesize: WenQuanYi Micro Hei Mono:pixelsize=16:antialias=true
!XTerm*faceNameDoublesize: WenQuanYi Micro Hei Mono:pixelsize=18:antialias=true:italic
XTerm*xftAntialias: true
XTerm*VT100.CjkWidth:false
! 
!解决Alt键不好使
!XTerm*eightBitInput:false
XTerm*altIsNotMeta:true
XTerm*altSendsEscape:true



